{
  "name"             : "sprout/seedling",
  "description"      : "Tenant-specific multi-database support for a Laravel Sprout",
  "type"             : "library",
  "require"          : {
    "php"              : "^8.3",
    "laravel/framework": "^12.46",
    "sprout/bud"       : "^2.0-dev"
  },
  "require-dev"      : {
    "phpunit/phpunit"    : "^11.0",
    "orchestra/testbench": "^10.0",
    "larastan/larastan"  : "^3.0"
  },
  "license"          : "MIT",
  "autoload"         : {
    "psr-4": {
      "Sprout\\Seedling\\": "src/"
    },
    "files": []
  },
  "autoload-dev"     : {
    "psr-4": {
      "Sprout\\Seedling\\Tests\\"       : "tests",
      "Workbench\\App\\"                : "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\"  : "workbench/database/seeders/"
    }
  },
  "authors"          : [
    {
      "name" : "Ollie Read",
      "email": "code@ollieread.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts"          : {
    "post-autoload-dump": [
      "@clear",
      "@prepare",
      "@php vendor/bin/testbench package:discover --ansi"
    ],
    "clear"             : "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare"           : "@php vendor/bin/testbench package:discover --ansi",
    "build"             : "@php vendor/bin/testbench workbench:build --ansi",
    "serve"             : [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve --ansi"
    ],
    "analyse"           : [
      "@php vendor/bin/phpstan analyse --memory-limit=2G --verbose --ansi"
    ],
    "test"              : [
      "@clear",
      "@prepare",
      "@build",
      "@php vendor/bin/phpunit --testsuite=Unit,Feature"
    ]
  },
  "extra"            : {
    "laravel"     : {
      "providers": [
        "Sprout\\Seedling\\SeedlingServiceProvider"
      ],
      "facades"  : []
    },
    "branch-alias": {}
  },
  "config"           : {
    "allow-plugins": {}
  }
}
